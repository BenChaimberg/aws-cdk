{
  "name": "@aws-cdk/sdk",
  "version": "1.20.0",
  "description": "AWS SDK used in CDK tools",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.sdk",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "cdk-sdk"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.SDK",
        "packageId": "Amazon.CDK.SDK",
        "signAssembly": true,
        "assemblyOriginatorKeyFile": "../../key.snk",
        "iconUrl": "https://raw.githubusercontent.com/aws/aws-cdk/master/logo/default-256-dark.png"
      },
      "python": {
        "distName": "aws-cdk.sdk",
        "module": "aws_cdk.sdk"
      }
    }
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "awslint": "cdk-awslint",
    "build+test+package": "npm run build+test && npm run package",
    "build+test": "npm run build && npm test",
    "compat": "cdk-compat"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "dependencies": {
    "aws-sdk": "^2.599.0"
  },
  "bundledDependencies": [
    "aws-sdk"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "statements": 80
      }
    },
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "html",
      "text-summary"
    ]
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/semver": "^6.2.0",
    "cdk-build-tools": "1.20.0",
    "jest": "^24.9.0",
    "pkglint": "1.20.0"
  },
  "repository": {
    "url": "https://github.com/aws/aws-cdk.git",
    "type": "git",
    "directory": "packages/@aws-cdk/sdk"
  },
  "keywords": [
    "aws",
    "cdk"
  ],
  "homepage": "https://github.com/aws/aws-cdk",
  "engines": {
    "node": ">= 10.3.0"
  },
  "stability": "experimental"
}
